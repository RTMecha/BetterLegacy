{
  // 0 = no drag, regardless of settings
  // 1 = optional drag
  // 2 = forced drag, regardless of settings
  "drag_mode": 1,
  "base": {
    "rect": {
      "anc_pos": [ 0, 0 ],
      "anc_max": [ 0.5, 0.5 ],
      "anc_min": [ 0.5, 0.5 ],
      "pivot": [ 0.5, 0.5 ],
      "size": [ 600, 400 ]
    },
    "theme_group": "Background_1"
  },
  "top_panel": {
    "rect": {
      "anc_pos": [ 0, 0 ],
      "anc_max": [ 1, 1 ],
      "anc_min": [ 0, 1 ],
      "pivot": [ 0, 0 ],
      "size": [ 32, 32 ]
    },
    "theme_group": "Background_1"
  },
  "title": {
    "rect": {
      "anc_pos": [ 2, 0 ],
      "anc_max": [ 1, 1 ],
      "anc_min": [ 0, 0 ],
      "pivot": [ 0.5, 0.5 ],
      "size": [ -12, -8 ]
    },
    "alignment": 3, // MiddleLeft
    "text": "Select a Font",
    "font_size": 20,
    "font_style": 0, // Normal
    "horizontal_overflow": 0, // Wrap
    "vertical_overflow": 1, // Overflow
    "theme_group": "Light_Text"
  },
  "search": {
    "rect": {
      "anc_pos": [ 0, 0 ],
      "anc_max": [ 1, 1 ],
      "anc_min": [ 0, 1 ],
      "pivot": [ 0.5, 1 ],
      "size": [ 0, 32 ]
    }
  },
  "scrollbar": {
    "rect": {
      "anc_pos": [ 0, 0 ],
      "anc_max": [ 1, 1 ],
      "anc_min": [ 1, 0 ],
      "pivot": [ 0, 0.5 ],
      "size": [ 32, 0 ]
    }
  },
  "layout": {
    "cell_size": [ 584.0, 32.0 ],
    "spacing": [ 0.0, 8.0 ],
    "constraint": 1,
    "constraint_count": 1,
    "padding": {
      "top": 40,
      "bottom": 4,
      "left": 8,
      "right": 8
    }
